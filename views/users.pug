extends layout

block content
	h1= title
	a(href="/addUser") Add User
	table.table.table-bordered
		thead
			tr 
				th Username
				th Create
				th Email
		tbody
			- for(var user in data)
				tr 
					td
						a(href="/user/"+data[user].key)=data[user].username
					td=data[user].createdAt
					td=data[user].email
